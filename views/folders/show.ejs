<%- include('../partials/header') %>
<thead>
  <h1><%= folder.title %></h1>
</thead>

<tbody>
  <button><a href="/folders/<%= folder._id %>/notes/new">+</a></button>
  <%if(notes.length){%>

  <% notes.forEach(function(m) { %>
    <tr>
      <br><td><a href="notes/<%= m._id %>"><%= m.title %></a></td>
    </tr>
    <% }); %>
    <% } else { %>
        <p>No Notes in this folder</p>
      <% } %>
  </tbody>

<%- include('../partials/footer') %>